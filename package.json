{
  "name": "avc-vietnam",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en-US",
    "start:vi": "ng serve --configuration=vi",
    "start:en-UK": "ng serve --configuration=en-UK",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng extract-i18n --output-path i18n && xliffmerge --profile xliffmerge.json en-US en-UK vi",
    "analyze": "ng build --prod --build-optimizer --stats-json && webpack-bundle-analyzer dist/avc-vietnam/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^4.0.7",
    "@angular/animations": "^11.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^11.2.4",
    "@angular/compiler": "^11.2.4",
    "@angular/core": "^11.2.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^11.2.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^11.2.4",
    "@angular/platform-browser-dynamic": "^11.2.4",
    "@angular/router": "^11.2.4",
    "@angular/service-worker": "^11.2.4",
    "@swimlane/ngx-charts": "^16.0.0",
    "lodash-es": "^4.17.21",
    "mat-table-exporter": "^10.2.3",
    "protractor-fail-fast": "^3.1.0",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "xlsx": "^0.16.9",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "^11.2.3",
    "@angular/compiler-cli": "^11.2.4",
    "@angular/language-service": "^11.2.4",
    "@angular/localize": "^11.2.4",
    "@types/jasmine": "^3.6.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.31",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
